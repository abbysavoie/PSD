        function AllowNumbersOnly(e) {
            var code = (e.which) ? e.which : e.keyCode;
            if (code > 31 && (code < 48 || code > 57)) {
              e.preventDefault();
            }
          }
        
        function showSecurityQuestions(){
            if($j("#usernameResetPassId").val() == ''){
                $j("#usernameResetPassId").css({"border":"1px solid red","display":"block"});
                $j("#usernameResetPassId-error").removeClass("hidden");
                $j("#usernameResetPassId-error").text("User Name is Required");
                return false;
            }
            else{
                $j("#usernameResetPassId").css({"border":"","display":"none"});
                $j("#usernameResetPassId-error").addClass("hidden");
                        
                Visualforce.remoting.Manager.invokeAction(
                    '{!$RemoteAction.RAD_LoginRegisterCtrl.getAllQuestionandAnswers}',
                    $j("#usernameResetPassId").val(),
                    function(result, event) {
                        if(event.status == true){
                            console.log(result);
                            securityQuestionandAnswers = result;
                            $j("#QId").removeClass("hidden");
                            $j("#UId").addClass("hidden");
                            //$j("#ResetPasswordCancelId").addClass("hidden");//rhathi 02 JUL 2017
                            $j("#ResetPasswordSubmitId").addClass("hidden");
                            $j("#ResetPasswordNextId").removeClass("hidden");
                            $j("#ResetloginButton").addClass("hidden");
                            
                            $j("#ques1Id").text(result[0].question);
                            $j("#ques2Id").text(result[1].question);
                            $j("#ques3Id").text(result[2].question);
                            $j("#ans1Id").text(result[0].answer);
                            
                            
                            
                        }
                        else{
                            $j("#usernameResetPassId").css({"border":"1px solid red","display":"block"});
                            $j("#usernameResetPassId-error").removeClass("hidden");
                            $j("#usernameResetPassId-error").text($j("#usernameResetPassId").val()+" not a valid User Name");
                            
                            return false;
                        }
                    }
                );
            }
        
        }
    

