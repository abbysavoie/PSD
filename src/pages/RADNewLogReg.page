        function calculateAge(){
            var mm = $j( "#HRmonthId" ).val();
            var dt = $j( "#dateday" ).val();
            var yy = $j( "#dateYear" ).val();
           
            if(mm !=0 && dt != 0 && yy != 0){
                var dob = yy+'-'+mm+'-'+dt;

                //alert(dob);
                $j("[id*='RmonthId']").val(dob); //store the date format -
                
                //now change the format for calculation
                dob = yy+'/'+mm+'/'+dt;
                dob = new Date(dob);
                //alert(dob);
                var today = new Date();
                var age = Math.floor((today-dob) / (365.25 * 24 * 60 * 60 * 1000));
                //alert(age);

                $j("#HRageId").val(age+'');
                $j("[id*='RageId']").val(age);
                document.getElementById("HRageId").disabled = true;
            }
            
            $j( "#HRmonthId" ).val(mm);
            
            
        
        }
  
              
        
