                 function papercheck()
                {
                    var text = j$('#HRemailId').val();
                    if(text == '' || text == null)
                    {
                        var checked = j$('#showPaperless').is(':checked');
                        if(checked==true)
                        {
                            var HRemailvalidId = document.getElementById('HRemailId');
                            var HRemailpaperlessEId  = document.getElementById('HREmailPaperless-error');
                            //alert('Email is  required when paperless checkbox is checked');
                            HRemailvalidId .style.cssText = "border: 1px solid red";
                            HRemailpaperlessEId.style.cssText = "display:block";
                            document.getElementById('HRemailId').focus();
                            return false;
                        }
                        else if(checked==false)
                            {
                            var HRemailvalidId = document.getElementById('HRemailId');
                            var HRemailpaperlessEId  = document.getElementById('HREmailPaperless-error');
                            HRemailvalidId.style.cssText = "";
                            HRemailpaperlessEId.style.cssText = "display:None";
                            
                            }
                    }
                    else
                    {
                        var HRemailvalidId = document.getElementById('HRemailId');
                        var HRemailpaperlessEId  = document.getElementById('HREmailPaperless-error');
                        HRemailvalidId.style.cssText = "";
                        HRemailpaperlessEId.style.cssText = "display:None";
                        return true;
                    }
                }  
                  j$('#showPaperless').on('click',function() {
                    var checked = j$(this).is(':checked');
                    if(checked==true)
                    {
                        j$("#paperless_add").show(); 
                        j$('#HRemailId').attr('disabled',false);
                        var text = j$('#HRemailId').val();
                        checktext(text);
                    }
                    else
                    {
                        j$("#paperless_add").hide();
                        var HRemailvalidId = document.getElementById('HRemailId');
                        var HRemailpaperlessEId  = document.getElementById('HREmailPaperless-error');
                        HRemailvalidId .style.cssText = "";
                        HRemailpaperlessEId.style.cssText = "display:None";
                    }
                });
                
                function checktext(text)
                {
                    var HRemailvalidId = document.getElementById('HRemailId');
                    var HRemailpaperlessEId  = document.getElementById('HREmailPaperless-error');
                    
                    if(text=='')
                    {
                        HRemailvalidId .style.cssText = "border: 1px solid red";
                        HRemailpaperlessEId.style.cssText = "display:block";
                        document.getElementById('HRemailId').focus();
                        return false;
                    }
                    else if(text!='')
                    {
                        HRemailvalidId .style.cssText = "";
                        HRemailpaperlessEId.style.cssText = "display:None";
                    }
                }
                       
        
